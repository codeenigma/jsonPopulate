---
# Defines a group of "pages", keyed by id. Each entry consists of:
# - source: path to a json file, relative to the root of the config file.
# - url: a web page to use as source for populating data.
dirs:
  - {source: "example", url: "https://www.codeenigma.com"}
  
# Boolean (0/1). Turn debug mode on/off. Currently only triggers twig debug.
debug: 1
